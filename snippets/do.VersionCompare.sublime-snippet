<snippet>
	<content><![CDATA[
\${VersionCompare} "${1:version1}" "${2:version2}" \$${3:result}

\${If} \$${3} = 0 # is equal

	${4:# your code here}

\${ElseIf} \$${3} = 1 # ${1} is newer

	${5:# your code here}

\${ElseIf} \$${3} = 2 # ${2} is newer

	${6:# your code here}

\${EndIf}
]]></content>
	<tabTrigger>do__VersionCompare</tabTrigger>
	<scope>source.nsis</scope>
</snippet>
